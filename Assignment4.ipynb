def decoding(node, bits):
    temp = node
    final = []
    for i in bits:
        if i == '1':
            temp = temp.left
        else:
            temp = temp.right
            
        if temp.left == None and temp.right == None:
            final.append(temp.symbol)
            temp = node
    print("".join(final))
